Host *
  ForwardAgent yes
  ServerAliveInterval 30
  ServerAliveCountMax 5
  GSSAPIAuthentication no

Host github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/gitkey

Host gitlab.com
  HostName gitlab.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/gitkey

Host 127.0.0.1
StrictHostKeyChecking no
UserKnownHostsFile=/dev/null

Host 192.168.0.0/16
StrictHostKeyChecking no
UserKnownHostsFile=/dev/null

Host 172.16.0.0/12
StrictHostKeyChecking no
UserKnownHostsFile=/dev/null

Host *.toshi.click
  User toshi
  IdentityFile ~/.ssh/toshi_sshkey

Host *.shigure.click
  User toshi
  IdentityFile ~/.ssh/toshi_sshkey

Host *.test
  User toshi
  IdentityFile ~/.ssh/toshi_sshkey
