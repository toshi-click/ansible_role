Host github.com
  HostName github.com
  AddKeysToAgent yes
  UseKeychain yes
  User git
  IdentityFile ~/.ssh/rsa_4096

Host gitlab.com
  HostName gitlab.com
  AddKeysToAgent yes
  UseKeychain yes
  User git
  IdentityFile ~/.ssh/rsa_4096

Host *
  UseKeychain yes
  AddKeysToAgent yes

ServerAliveInterval 30
